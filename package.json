{
  "name": "Yassss",
  "version": "1.0.0",
  "description": "Yet another simple static site starter (for Jekyll)",
  "license": "MIT",
  "engines": {
    "node": "12.14.0"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "bundle exec jekyll build",
    "clean": "rimraf _site && rimraf dist",
    "lint": "npm run lint:js; npm run lint:css",
    "lint:eslint": "eslint './_assets/javascript/**/*'",
    "lint:stylelint": "stylelint './_assets/css/**/*'",
    "lint:js": "npm run lint:eslint",
    "lint:css": "npm run lint:stylelint",
    "lint:staged": "lint-staged",
    "preinstall": "bundle install",
    "start": "bundle exec jekyll serve",
    "test": "npm run lint"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.js.liquid": "eslint",
    "*.scss": "stylelint",
    "*.scss.liquid": "stylelint",
    "*.css": "stylelint"
  },
  "pre-commit": "lint:staged",
  "repository": {
    "type": "git",
    "url": "git://github.com/gblakeman/yassss.git"
  },
  "bugs": {
    "url": "https://github.com/gblakeman/yassss"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "overrides": {
      "files": "_assets/javascript/*.js.liquid"
    },
    "plugins": [
      "jsx-a11y"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "arrow-parens": [
        "error",
        "always"
      ],
      "arrow-body-style": [
        2,
        "as-needed"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "function-paren-newline": 0,
      "import/first": 0,
      "import/newline-after-import": 0,
      "import/no-dynamic-require": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-named-as-default": 0,
      "import/no-unresolved": 2,
      "import/no-webpack-loader-syntax": 0,
      "import/prefer-default-export": 0,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "jsx-a11y/anchor-is-valid": 0,
      "jsx-a11y/aria-props": 2,
      "jsx-a11y/heading-has-content": 0,
      "jsx-a11y/label-has-for": 2,
      "jsx-a11y/mouse-events-have-key-events": 2,
      "jsx-a11y/role-has-required-aria-props": 2,
      "jsx-a11y/role-supports-aria-props": 2,
      "max-len": 0,
      "newline-per-chained-call": 0,
      "no-confusing-arrow": 0,
      "no-console": 2,
      "no-restricted-globals": 0,
      "no-use-before-define": 0,
      "object-curly-newline": 0,
      "object-shorthand": [
        "error",
        "consistent"
      ],
      "one-var": ["error", { "let": "consecutive" }],
      "one-var-declaration-per-line": 0,
      "prefer-destructuring": 0,
      "prefer-template": 2,
      "class-methods-use-this": 0,
      "require-yield": 0,
      "semi": [
        "error",
        "never"
      ],
      "switch-colon-spacing": [
        "error",
        {
          "after": false,
          "before": true
        }
      ]
    }
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "del": "^2.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.1",
    "eslint-plugin-redux-saga": "^0.8.0",
    "lint-staged": "^7.1.0",
    "pre-commit": "^1.2.2",
    "redux-saga": "^0.15.3",
    "rimraf": "^2.4.3",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.4.1",
    "yargs": "^4.4.0"
  }
}
